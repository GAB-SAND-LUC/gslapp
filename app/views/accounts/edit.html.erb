
<br><br><div>
<%=form_for @account, url: account_path do |f| %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :phone %>
  <%= f.telephone_field :phone  %>
  <%= f.label :address %>
  <%= f.text_field  :address  %>
  <%= f.label :city %>
  <%= f.text_field  :city %>
  <%= f.submit %>
<% end %>
</div>